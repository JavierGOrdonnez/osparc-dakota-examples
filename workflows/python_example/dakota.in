
    environment
        tabular_data
            tabular_data_file = 'results.dat'
    
        method
            id_method = 'SAMPLING'
            sample_type
                lhs
            sampling
                samples = 10
                seed = 1234
            
        
        model
            id_model = 'TRUE_MODEL'
            single
                interface_pointer = 'INTERFACE'
                variables_pointer = 'VARIABLES'
                responses_pointer = 'RESPONSES'
        

        variables
            continuous_design = 3
            id_variables = 'VARIABLES'
                descriptors 'X0' 'X1' 'X2'
        
                lower_bounds      0 0 0
                
                upper_bounds      1 1 1
                

        responses
            id_responses = 'RESPONSES'
            descriptors 'SUM' 'MULT'
            objective_functions = 2
            no_gradients
            no_hessians
        
        interface,
            id_interface = 'INTERFACE' 
            
            python 
                analysis_drivers
                    'evaluator'
            
        